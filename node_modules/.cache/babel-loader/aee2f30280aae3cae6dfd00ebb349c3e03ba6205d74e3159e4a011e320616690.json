{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PostForm from \"@/components/PostForm\";\nimport PostList from \"@/components/PostList\";\nimport MyDialog from \"@/components/UI/MyDialog\";\nimport axios from 'axios';\nexport default {\n  components: {\n    MyDialog,\n    PostList,\n    PostForm\n  },\n  data() {\n    return {\n      posts: [],\n      title: '',\n      body: '',\n      dialogVisible: false\n    };\n  },\n  methods: {\n    CreatePost(post) {\n      this.posts.push(post);\n      this.dialogVisible = false;\n    },\n    RemovePost(post) {\n      this.posts = this.posts.filter(p => p.id !== post.id);\n    },\n    showDialog() {\n      this.dialogVisible = true;\n    },\n    async fetchPost() {\n      try {\n        const response = await axios.get('https://jsonplaceholder.typicode.com/posts?_limit=10');\n        console.log(response.data);\n        this.posts = response.data;\n      } catch (e) {\n        alert('Ошибка');\n      }\n    }\n  },\n  mounted() {\n    this.fetchPost();\n  }\n};","map":{"version":3,"names":["PostForm","PostList","MyDialog","axios","components","data","posts","title","body","dialogVisible","methods","CreatePost","post","push","RemovePost","filter","p","id","showDialog","fetchPost","response","get","console","log","e","alert","mounted"],"sources":["C:\\Users\\Iana\\Desktop\\blatttt\\newapp\\src\\App.vue"],"sourcesContent":["<template>\r\n    <div class=\"app\">\r\n        <h1>Страница с постами: </h1>\r\n        <br>\r\n        <my-button @click=\"showDialog\" style=\"margin-top: 15px;\">Создать пост</my-button>\r\n    <my-dialog v-model:show=\"dialogVisible\">\r\n        <post-form @create=\"CreatePost\"/>\r\n    </my-dialog>\r\n    <post-list\r\n            :posts=\"posts\"\r\n            @remove=\"RemovePost\"\r\n            v-if=\"!isPostLoading\"\r\n    />\r\n        <div></div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import PostForm from \"@/components/PostForm\";\r\n    import PostList from \"@/components/PostList\";\r\n    import MyDialog from \"@/components/UI/MyDialog\";\r\n    import axios from 'axios'\r\n    export default {\r\n        components: {\r\n            MyDialog,\r\n            PostList,\r\n            PostForm\r\n        },\r\n        data() {\r\n            return{\r\n                posts: [],\r\n                title: '',\r\n                body: '',\r\n                dialogVisible: false,\r\n            }\r\n        },\r\n        methods: {\r\n            CreatePost(post) {\r\n                this.posts.push(post)\r\n                this.dialogVisible = false\r\n            },\r\n            RemovePost(post) {\r\n                this.posts = this.posts.filter(p => p.id !== post.id)\r\n            },\r\n            showDialog() {\r\n                this.dialogVisible = true\r\n            },\r\n            async fetchPost() {\r\n                try {\r\n                    const response = await axios.get('https://jsonplaceholder.typicode.com/posts?_limit=10')\r\n                    console.log(response.data)\r\n                    this.posts = response.data\r\n                } catch (e) {\r\n                    alert('Ошибка')\r\n                }\r\n            }\r\n        },\r\n        mounted() {\r\n            this.fetchPost()\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n}\r\n\r\nform {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n    .app {\r\n        padding: 20px;\r\n    }\r\n\r\n</style>"],"mappings":";AAkBI,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,QAAO,MAAO,0BAA0B;AAC/C,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,UAAU,EAAE;IACRF,QAAQ;IACRD,QAAQ;IACRD;EACJ,CAAC;EACDK,IAAIA,CAAA,EAAG;IACH,OAAM;MACFC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,aAAa,EAAE;IACnB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAACC,IAAI,EAAE;MACb,IAAI,CAACN,KAAK,CAACO,IAAI,CAACD,IAAI;MACpB,IAAI,CAACH,a